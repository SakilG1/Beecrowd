#include<bits/stdc++.h>
using namespace std;
int main()
{
    int N,A,B,i,p1,p2;
    while(true)
    {
        cin >> N;
        if(N==0)
            break;
        p1=p2=0;
        for(i=1; i<=N; i++)
        {
            cin >> A >> B;
            if(A>B)
            {
                p1++;
            }
            else if(B>A)
            {
                p2++;
            }
        }
        cout << p1 << " " << p2 << "\n";
    }

    return 0;
}
