#include<bits/stdc++.h>
using namespace std;
int main()
{
    double arr[100], x;
    int i;
    cin >> x;
    for(i=0; i<100; i++)
    {
        arr[i]=x;
        x /= 2;
    }
    for(i=0; i<100; i++)
    {
        cout << "N[" << i << "] = " << fixed << setprecision(4) << arr[i] << endl;
    }
    return 0;
}
