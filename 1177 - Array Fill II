#include<bits/stdc++.h>
using namespace std;
int main()
{
    int T,N[1000],i,j=0;

    cin >> T;

    for(i=0; i<1000; i++)
    {
        N[i]=j;
        j++;
        if(j==T)
            j=0;
    }
    for(i=0; i<1000; i++)
    {
    cout << "N[" << i << "] = " << N[i] << "\n";
    }

    return 0;
}
