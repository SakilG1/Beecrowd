#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[100],v,p,i;

    for(i=0; i<100; i++)
    {
        cin >> arr[i];
    }
    v=arr[1];
    p=1;
    for(i=0; i<100; i++)
    {
        if(v<arr[i])
        {
            v=arr[i];
            p=i+1;
        }
    }
    cout << v << "\n";
    cout << p << "\n";
    

    return 0;
}
