#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,bra,gar;
    double x;
    while(cin >> n)
    {
        if(n==0)
            break;
        else{
            x=n/90.0;
            bra=floor(x);
            gar=ceil(x*7);
        }
        cout << "Brasil " << bra << " x Alemanha " << gar << endl;
    }

    return 0;
}
