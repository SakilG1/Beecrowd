#include<bits/stdc++.h>
using namespace std;
int main()
{
    float A,B,C,r1,r2,x;
    cin >> A >> B >> C;
    x=(B*B)-(4*A*C);
     if(x<0)
     {
         cout << "Impossivel calcular\n";
     }
     else if(A==0)
     {
         cout << "Impossivel calcular\n";
     }
    else{
            r1= (-B+sqrt(x))/(2*A);
            r2= (-B-sqrt(x))/(2*A);
        printf("R1 = %.5f\n",r1);
        printf("R2 = %.5f\n",r2);
    }
    return 0;
}
