#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x, i=5, ev=0, od=0, neg=0, pos=0;

    while(i--)
    {
        cin >> x;
        if(x>0)
            pos++;
        else if(x<0)
            neg++;
        if(x%2==0)
            ev++;
        else
            od++;
    }

    cout << ev << " valor(es) par(es)\n" << od << " valor(es) impar(es)\n" << pos << " valor(es) positivo(s)\n" << neg << " valor(es) negativo(s)\n";

    return 0;
}
