#include<bits/stdc++.h>
using namespace std;
int main()
{
    int T,i,arr[10],N,j;

    cin >> T;

    for(i=0; i<T; i++)
    {
        cin >> N;
        for(j=0; j<N; j++)
        {
            cin >> arr[j];
        }
    cout << "Case " << i+1 << ": " << arr[N/2] << "\n";

    }

    return 0;
}
