#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a, g, d;
    a=g=d=0;
    while(1)
    {
        int x;
        cin >> x;
        if(x==1)
            a++;
        else if(x==2)
            g++;
        else if(x==3)
            d++;
        else if(x==4)
            break;
        else
            continue;
    }
    cout << "MUITO OBRIGADO\n" << "Alcool: " << a << "\nGasolina: " << g << "\nDiesel: " << d << endl;

    return 0;
}
