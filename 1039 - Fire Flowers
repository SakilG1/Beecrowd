#include<bits/stdc++.h>
using namespace std;
int main()
{
    int R1,X1,Y1,R2,X2,Y2;
    float distance;
    while( cin >> R1 >> X1 >> Y1 >> R2 >> X2 >> Y2)
    {
        distance= sqrt(pow(X2-X1,2)+pow(Y2-Y1,2));
        if(R1>=distance+R2)
        {
            cout << "RICO\n";
        }
        else{
            cout << "MORTO\n";
        }
    }

    return 0;
}
