#include<bits/stdc++.h>
using namespace std;
int main()
{
    int N,X,i;
    cin >> N;
    for(i=1; i<=N; i++)
    {
        cin >> X;
        if(X>0)
        {
            if(X%2==0)
            {
                cout << "EVEN POSITIVE\n";
            }
            else{
                cout << "ODD POSITIVE\n";
            }
        }
        else if(X==0)
        {
            cout << "NULL\n";
        }
        else{
            if(X%2==0)
            {
                cout << "EVEN NEGATIVE\n";
            }
            else{
                cout << "ODD NEGATIVE\n";
            }
        }
    }

    return 0;
}
