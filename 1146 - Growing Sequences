#include<bits/stdc++.h>
using namespace std;
int main()
{
    int N,X,i;
    while(true)
    {
        cin >> X;
        if(X==0)
            break;
        else
        {
            for(i=1; i<X; i++)
            {
                cout << i << " ";
            }
            cout << X << "\n";
        }
    }

    return 0;
}
