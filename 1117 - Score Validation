#include<bits/stdc++.h>
using namespace std;
int main()
{
    double x, s=0, a=0;
    while(cin >> x)
    {
        if(x>10 || x<0)
        {
            printf("nota invalida\n");
        }
        else{
            s += x;
            a++;
            if(a==2)
            {
                cout << "media = " << fixed << setprecision(2) << s/2.0 << endl;
                break;
            }
        }
    }

    return 0;
}
