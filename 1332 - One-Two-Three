#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,x;
    cin >> n;
    while(n--)
    {
        string str;
        cin >> str;
        x=str.length();
        if(x==5)
        {
            cout << "3\n";
        }
        else{
            if((str[0]=='o' && str[1]=='n') || (str[0]=='o' && str[2]=='e') || (str[1]=='n' && str[2]=='e'))
            {
                cout << "1\n";
            }
            else{
                cout << "2\n";
            }
        }

    }

    return 0;
}
